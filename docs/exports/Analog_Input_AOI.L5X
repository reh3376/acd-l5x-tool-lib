</Rung>
<Rung Number="2" Type="N">
<Text>
<![CDATA[XIC(HMI_Load_LoRaw)MOV(In_Raw,Cfg_RawMin);]]>
</Text>
</Rung>
<Rung Number="3" Type="N">
<Text>
<![CDATA[[XIO(Cfg_PreScaledPV) [CPT(L_ComputedValue,((In_Raw - Cfg_RawMin)/(Cfg_RawMax - Cfg_RawMin))*(Cfg_EUMax - Cfg_EUMin) + Cfg_EUMin) ,MOV(L_ComputedValue,L_ScaledValue) ] ,XIC(Cfg_PreScaledPV) MOV(In_PreScaledPV,L_ScaledValue) ,XIO(CfgHoldFaultVal) XIO(PVInRange) MOV(CfgUDefFaultVal,L_ScaledValue) ];]]>
</Text>
</Rung>
<Rung Number="4" Type="N">
<Text>
<![CDATA[[XIO(Cfg_PreScaledPV) LEQ(L_ComputedValue,L_URFaultSetpoint) ,XIC(Cfg_PreScaledPV) LEQ(In_PreScaledPV,L_URFaultSetpoint) ]OTE(L_PVUnderRange);]]>
</Text>
</Rung>
<Rung Number="5" Type="N">
<Text>
<![CDATA[[XIO(Cfg_PreScaledPV) GEQ(L_ComputedValue,L_ORFaultSetpoint) ,XIC(Cfg_PreScaledPV) GEQ(In_PreScaledPV,L_ORFaultSetpoint) ]OTE(L_PVOverRange);]]>
</Text>
</Rung>
<Rung Number="6" Type="N">
<Text>
<![CDATA[[XIO(Emulation_Mode) LEQ(L_ScaledPV,L_URFaultSetpoint) ,XIC(Emulation_Mode) XIC(L_PVUnderRange) ]OTE(Sts_URFault);]]>
</Text>
</Rung>
<Rung Number="7" Type="N">
<Text>
<![CDATA[[XIO(Emulation_Mode) GEQ(L_ScaledPV,L_ORFaultSetpoint) ,XIC(Emulation_Mode) XIC(L_PVOverRange) ]OTE(Sts_ORFault);]]>
</Text>
</Rung>
<Rung Number="8" Type="N">
<Text>
<![CDATA[XIO(L_PVUnderRange)XIO(L_PVOverRange)OTE(PVInRange);]]>
</Text>
</Rung>
<Rung Number="9" Type="N">
<Comment>
<![CDATA[Emulation Mode Enable Logic]]>
</Comment>
<Text>
<![CDATA[XIC(Emulation_Enable)XIC(Config_Emulation_Mode)OTE(Emulation_Mode);]]>
